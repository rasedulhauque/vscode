        {
            "id": "d6916db9-2143-4704-bb7c-555437823893",
            "enabled": true,
            "name": "My jira2.workday.com Issues",
            "query": "assignee = currentUser() AND resolution = Unresolved ORDER BY lastViewed DESC",
            "siteId": "jira2.workday.com",
            "monitor": true
        },
        {
            "id": "7fa26d34-ef9d-4950-b664-7f8050b62ae2",
            "siteId": "jira2.workday.com",
            "name": "Recent",
            "query": "project = PBUPRODENG and assignee  = currentUser() and (not status in (Closed, Resolved))",
            "enabled": true,
            "monitor": true
        },
        {
            "id": "0c829b5f-69d3-423c-a905-281b11ced382",
            "siteId": "jira2.workday.com",
            "name": "Current Sprint",
            "query": "project = PBUPRODENG and assignee  = artem.veremey and (not status in (Closed, Resolved)) AND sprint in openSprints()",
            "enabled": true,
            "monitor": true
        },
        {
            "id": "49612efe-5850-4cd2-ac00-8bc8e3d08301",
            "siteId": "jira2.workday.com",
            "filterId": 204150,
            "name": "PBU-Urgent-Important",
            "query": "labels = pbu_urgent_important AND status not in (Closed, Resolved) OR labels = pbu_urgent_important AND created > -30d ORDER BY status ASC",
            "enabled": true,
            "monitor": true
        },
        {
            "id": "af19da13-9ef7-4e53-8a78-2b46e109aa46",
            "siteId": "jira2.workday.com",
            "name": "CMs this Week",
            "query": "project = PBUTECHOPS and type = Change and created > startOfWeek()",
            "enabled": true,
            "monitor": true
        }
    ],
