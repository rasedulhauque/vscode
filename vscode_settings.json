{
    "workbench.colorTheme": "Default Dark Modern",
    "workbench.iconTheme": "vscode-great-icons",
    "workbench.sideBar.location": "left",
    "editor.wordWrap": "on",
    "editor.rulers": [
        80,
        100,
        120
    ],
    //"files.trimTrailingWhitespace": true,
    //"files.trimFinalNewlines": true,
    "files.insertFinalNewline": true,
    "workbench.editor.enablePreview": false,
    "workbench.editor.tabSizing": "shrink",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.find.autoFindInSelection": "never",
    "solargraph.checkGemVersion": false,
    "ruby.codeCompletion": "rcodetools",
    "[ruby]":{
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "editor.autoIndent": "full"
    },
    "[go]": {
        "editor.insertSpaces": false,
        "editor.tabSize": 4
    },
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "none",
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.formatOnPaste": true
    },
    "[python]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4,
        "editor.formatOnSave": true
    },
    "python.linting.enabled": true,
    "python.linting.flake8Enabled": false,
    "python.formatting.provider": "black",
    "python.linting.pylintEnabled": true,
    "python.linting.pylintCategorySeverity.refactor": "Warning",
    "python.linting.pylintCategorySeverity.convention": "Warning",
    "python.linting.pylintArgs": [
        "--disable=C0301,C0111,E0402,W0702,W0108,W0703"
    ],
    "python.linting.banditEnabled": true,
    "python.linting.banditArgs": [
        "-x",
        "./tests",
        "-r",
    ],
    "python.linting.mypyEnabled": true,
    "python.linting.pycodestyleEnabled": true,
    "python.linting.pycodestyleArgs": [
        "--ignore E501"
    ],
    "python.testing.pytestArgs": [
        "-s",
        "-vvvv",
    ],
    "python.languageServer": "Pylance",
    "markdownlint.config": {
        "MD013": {
            "line_length": 120,
            "tables": false,
            "code_blocks": false,
        },
        "MD025": false,
        "MD033": false,
        "MD036": false,
        "MD041": false,
    },
    "notebook.displayOrder": [
        "text/html",
        "text/plain",
        "image/png"
    ],
    "workbench.startupEditor": "newUntitledFile",
    "docker.showExplorer": false,
    //"docker.context": "containerd1",
    "editor.minimap.enabled": false,
    "spellright.language": ["en"],
    "explorer.openEditors.visible": 9,
    "go.testFlags": [
        "-v",
        "-count=1"
    ],
    "go.lintTool":"golangci-lint",
    "go.lintOnSave": "workspace",
    "toggle.settingTitle": "go.coverOnSave",
    "toggle.settingState1": "true",
    "toggle.settingState2": "false",
    "go.coverOnSave": true,
    //"go.coverOnTestPackage": true,
    "go.coverMode": "set",
    "go.coverageOptions": "showUncoveredCodeOnly",
    "go.coverageDecorator": {
        "type": "highlight",
        "coveredHighlightColor": "rgba(64,128,128,0.05)",
        "uncoveredHighlightColor": "rgba(128,64,64,0.05)",
        "coveredGutterStyle": "blockgreen",
        "uncoveredGutterStyle": "blockred"
    },
    //"go.coverOnSingleTest": true,
    "workbench.panel.location": "bottom",
    "gitlens.gitExplorer.branches.layout": "list",
    "gitlens.keymap": "alternate",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
    },
    "editor.renderWhitespace": "all",
    "terraform.indexing": {
        "enabled": false,
        "liveIndexing": false,
        "delay": 500,
        "exclude": [
            ".terraform/*/",
            "*/.terraform/*/*"
        ]
    },
    "gitlens.historyExplorer.enabled": true,
    "jsonnet.libPaths": [
        "${workspaceFolder}",
        "${workspaceFolder}/vendor"
    ],
    "gitlens.views.repositories.branches.layout": "list",
    "gitlens.views.fileHistory.enabled": true,
    //"telemetry.enableTelemetry": false,
    "code-runner.enableAppInsights": false,
    "gitlens.advanced.telemetry.enabled": false,
    //"telemetry.enableCrashReporter": false,
    "go.testTimeout": "600s",
    "gopls": {
        "analyses": {
           "simplifycompositelit": false,
        }
    },
    //"testExplorer.useNativeTesting": false,
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
